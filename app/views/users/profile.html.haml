%section.clearfix.inner_body
  .pc.cm
    .p10.fl.w100
      %figure.profile_pic.round.img_fill
        = image_tag @user.profile_cover.picture.url(:medium)
      %figure.profile_cover.img_fill.fl.w100
        = image_tag @user.cover_photo.picture.url
        %hgroup.head_row.bs
          %h2= @user.user_name
        .info_row
          .info_cell.bs
            %i.pe-7s-albums
            %span= @user.created_albums.count
          .info_cell.bs
            %i.pe-7s-photo-gallery
            %span= @user.total_photos.count
          .info_cell.bs
            %i.pe-7s-like
            %span= @user.total_likes
          .info_cell.bs
            %i.pe-7s-chat
            %span= @user.total_comments
    %form.search_row.fl.w100.p10
      .fl.select
        %select
          %option Card View
          %option List View
      .fl.input
        %input{:placeholder => "Search ...", :type => "text"}/
      .btn_box.fr
        %button{:type => "submit"}
          %i.pe-7s-search
